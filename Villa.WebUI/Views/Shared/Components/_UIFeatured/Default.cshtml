@model ResultFeatureDto

<div class="featured section">
    <div class="container">
        <div class="row">
            <div class="col-lg-4">
                <div class="left-image">
                    <img src="@Model.ImageUrl" alt="">
                   @*  <a href="property-details.html"><img src="~/VillaAgency/assets/images/featured-icon.png" alt="" style="max-width: 60px; padding: 0px;"></a> *@
                </div>
            </div>
            <div class="col-lg-5">
                <div class="section-heading">
                    <h5>Özellikler</h5>
                    <h2>@Model.Title</h2>
                    
                </div>
                @await Component.InvokeAsync("_UIFAQ")
            </div>
            <div class="col-lg-3">
                <div class="info-table">
                    <ul>
                        <li>
                            <img src="~/VillaAgency/assets/images/info-icon-01.png" alt="" style="max-width: 52px;">
                            <h4>@Model.Square m2<br><span>Total Flat Space</span></h4>
                        </li>
                        <li>
                            <img src="~/VillaAgency/assets/images/info-icon-02.png" alt="" style="max-width: 52px;">
                            <h4>@Model.Contract<br><span>Contract Ready</span></h4>
                        </li>
                        <li>
                            <img src="~/VillaAgency/assets/images/info-icon-03.png" alt="" style="max-width: 52px;">
                            <h4>@Model.Payment<br><span>Payment Process</span></h4>
                        </li>
                        <li>
                            <img src="~/VillaAgency/assets/images/info-icon-04.png" alt="" style="max-width: 52px;">
                            <h4>@Model.Safety<br><span>24/7 Under Control</span></h4>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>