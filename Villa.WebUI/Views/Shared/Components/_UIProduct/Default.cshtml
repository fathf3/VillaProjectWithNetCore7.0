@model List<ResultProductDto>
<div class="properties section" id="urunler">
    <div class="container">
        <div class="row">
            <div class="col-lg-4 offset-lg-4">
                <div class="section-heading text-center">
                    <h6>| Properties</h6>
                    <h2>We Provide The Best Property You Like</h2>
                </div>
            </div>
        </div>
        <div class="row">
            @foreach(var item in Model)
            {
                <div class="col-lg-4 col-md-6">
                    <div class="item">
                        <a href="property-details.html"><img src="@item.ImageUrl" alt=""></a>
                        <span class="category">@item.Category</span>
                        <h6>$1.000.000</h6>
                        
                        <ul>
                            <li>Bedrooms: <span>@item.BedRoomCount</span></li>
                            <li>Bathrooms: <span>@item.BathRoomCount</span></li>
                            <li>Area: <span>@item.Area</span></li>
                            <li>Floor: <span>@item.Floor</span></li>
                            <li>Parking: <span>@item.ParkingCount spots</span></li>
                        </ul>
                        <div class="main-button">
                            <a href="#">Schedule a visit</a>
                        </div>
                    </div>
                </div>
            }
          
         
        </div>
    </div>
</div>