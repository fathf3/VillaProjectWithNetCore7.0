@model UpdateVideoDto
@{
    ViewData["Title"] = "Video Güncelle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="col-lg-12">
    <div class="card">
        <div class="mt-3">
            <form method="post">
                <div asp-validation-summary="All" class="text-danger mb-2"></div>
                <label>Video Url</label>
                <input type="text" asp-for="VideoUrl" class="form-control" />
                <br />


                <button class="btn btn-outline-dark">Kaydet</button>
            </form>
        </div>
    </div>
</div>